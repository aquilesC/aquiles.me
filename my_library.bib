@article{vandergrein2019,
  title = {Picornavirus Infection Induces Temporal Release of Multiple Extracellular Vesicle Subsets That Differ in Molecular Composition and Infectious Potential},
  author = {{van der Grein}, Susanne G. and Defourny, Kyra A. Y. and Rabouw, Huib H. and Galiveti, Chenna R. and Langereis, Martijn A. and Wauben, Marca H. M. and Arkesteijn, Ger J. A. and {van Kuppeveld}, Frank J. M. and {Nolte-`t Hoen}, Esther N. M.},
